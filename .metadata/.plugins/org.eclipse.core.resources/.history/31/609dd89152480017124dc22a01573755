public class Main {
	public static void main(String[] args){
		Sparrow.sayHello();
		//Bird.privateMethod(); NOT VISIBLE
		//System.out.println(Bird.privateField); NOT VISIBLE
		Sparrow.wingFlap();
		
	}
}
